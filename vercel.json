{
    "version": 2,

    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python3",
        "config": {
          "pythonVersion": "3.9.12"
        }
      }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "api/index.py"
        }
      ]
  }
  